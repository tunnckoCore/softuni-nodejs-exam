{
  "name": "softuni-nodejs-exam",
  "version": "0.1.0",
  "description": "Scaffold project for Software University Bulgaria. Final exam of Nodejs Development Course.",
  "repository": "tunnckoCore/softuni-nodejs-exam",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100 --branches 100 --statements 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "precommit": "git add --all",
    "commit": "git-cz"
  },
  "dependencies": {
    "bel": "^4.5.0",
    "data-store": "^0.16.1",
    "extend-shallow": "^2.0.1",
    "koa": "^2.0.0",
    "koa-bel": "^1.0.0",
    "koa-better-body": "^3.0.2",
    "koa-resource-router": "^0.4.0"
  },
  "devDependencies": {
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.12",
    "cz-conventional-changelog": "^1.2.0",
    "mukla": "^0.4.1",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.0.0",
    "standard-version": "^2.4.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
